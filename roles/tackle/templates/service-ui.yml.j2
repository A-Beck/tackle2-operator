---
apiVersion: v1
kind: Service
metadata:
  labels:
    app.kubernetes.io/name: {{ ui_service_name }}
    app.kubernetes.io/component: {{ ui_component_name }}
    app.kubernetes.io/part-of: {{ app_name }}
  name: {{ ui_service_name }}
  namespace: {{ app_namespace }}
spec:
  ports:
    - name: ui
      port: {{ ui_port }}
      targetPort: {{ ui_port }}
      protocol: TCP
  selector:
    app.kubernetes.io/name: {{ ui_service_name }}
    app.kubernetes.io/component: {{ ui_component_name }}
    app.kubernetes.io/part-of: {{ app_name }}
